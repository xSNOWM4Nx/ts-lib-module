{
  "name": "@danielneuweiler/ts-lib-module",
  "version": "0.1.0-alpha.1",
  "description": "This project represents my standard library for generic type definitions and patterns that I use in various other projects.",
  "author": "Daniel Neuweiler",
  "license": "MIT",
  "repository": "xSNOWM4Nx/ts-lib-module",
  "homepage": "https://github.com/xSNOWM4Nx/ts-lib-module",
  "main": "build/src/index.js",
  "types": "build/src/index.d.ts",
  "files": [
    "build/src"
  ],
  "keywords": [],
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "check": "gts check",
    "clean": "gts clean",
    "compile": "tsc -p . && npm.cmd run copy:packagedata && npm.cmd run copy:styles",
    "fix": "gts fix",
    "prepare": "npm.cmd run compile",
    "pretest": "npm.cmd run compile",
    "posttest": "npm.cmd run check",
    "devcompile": "npm.cmd run compile && npm.cmd run copy2:react-lib-module && npm.cmd run copy2:react-sky-map",
    "copy:packagedata": "copyfiles package.json README.md build/src",
    "copy:styles": "copyfiles -u 1 src/styles/* build/src",
    "copy2:react-lib-module": "copyfiles -u 2 build/src/**/* ./../react-lib-module/node_modules/@danielneuweiler/ts-lib-module",
    "copy2:react-sky-map": "copyfiles -u 2 build/src/**/* ./../react-flight-tracker/node_modules/@danielneuweiler/ts-lib-module"
  },
  "devDependencies": {
    "gts": "^1.1.2",
    "typescript": "^3.8.3",
    "@types/node": "^13.7.7"
  }
}